{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "test"
        ]
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": ["{projectRoot}/build", "{projectRoot}/public/build"]
    },
    "dev": {
      "dependsOn": [
        "^build"
      ]
    }
  }
}
